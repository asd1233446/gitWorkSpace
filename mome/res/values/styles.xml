<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="line_horizontal">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/common_line_height</item>
        <!--
        <item name="android:layout_marginLeft">@dimen/common_marginOrpadding</item>
        <item name="android:layout_marginRight">@dimen/common_marginOrpadding</item>
        -->
        <item name="android:background">@color/gary_line</item>
    </style>

    <style name="line_horizontal_Max">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/common_line_height_max</item>
        <!--
        <item name="android:layout_marginLeft">@dimen/common_marginOrpadding</item>
        <item name="android:layout_marginRight">@dimen/common_marginOrpadding</item
        -->
>        <item name="android:background">@color/combackgroud</item>
    </style>

    <style name="line_vercal">
        <item name="android:layout_width">@dimen/common_line_height</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@color/gary_line</item>
    </style>

    <style name="listView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:divider">@null</item>
    </style>

</resources>